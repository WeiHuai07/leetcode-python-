class Solution:
    def search(self, nums: List[int], target: int) -> bool:
        if target>=nums[0]:
            for i in range(len(nums)):
                if target==nums[i]:
                    return True
                
        else:
            for i in range(len(nums)-1,-1,-1):
                if target==nums[i]:
                    return True
        return False

